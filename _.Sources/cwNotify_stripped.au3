EndIf
StringRegExp($s_String, $s_RE_G_Object_End, 1, $i_OsC)
If Not @error Then
$i_OsC = @extended
Else
Do
$a_T = StringRegExp($s_String, $s_RE_G_Object_Key, 1, $i_OsC)
If @error Then Return SetError(2, $i_OsC, "")
$i_OsC = @extended
$s_Key = __JSON_ParseString($a_T[0])
$o_Value = _JSON_Parse($s_String, $bUseMaps, $i_OsC)
If @error Then Return SetError(3, $i_OsC, "")
$i_OsC = @extended
If $bUseMaps Then
$o_Current[$s_Key] = $o_Value
Else
$o_Current($s_Key) = $o_Value
EndIf
StringRegExp($s_String, $s_RE_G_Object_Further, 1, $i_OsC)
If Not @error Then
$i_OsC = @extended
ContinueLoop
Else
StringRegExp($s_String, $s_RE_G_Object_End, 1, $i_OsC)
If Not @error Then
$i_OsC = @extended
ExitLoop
Else
Return SetError(4, $i_OsC, "")
