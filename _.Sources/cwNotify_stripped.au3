ReDim $o_Current[$d_N]
EndIf
$o_Current[$i_C] = $o_Value
$i_C += 1
StringRegExp($s_String, $s_RE_G_Object_Further, 1, $i_OsC)
If Not @error Then
$i_OsC = @extended
