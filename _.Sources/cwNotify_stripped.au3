If $hItem_tmp Then $hItem = $hItem_tmp
EndIf
EndIf
If Not IsHWnd($hWnd) Then $hWnd = GUICtrlGetHandle($hWnd)
If $bExpand Then
__GUICtrlTreeView_ExpandItem($hWnd, $TVE_EXPAND, $hItem)
