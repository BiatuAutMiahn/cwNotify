$tText = DllStructCreate("wchar TipText[" & $iBuffer & "]")
$iMsg = $SB_SETTIPTEXTW
Else
$tText = DllStructCreate("char TipText[" & $iBuffer & "]")
$iMsg = $SB_SETTIPTEXTA
EndIf
DllStructSetData($tText, "TipText", $sText)
__GUICtrl_SendMsg($hWnd, $iMsg, $iPart, $tText)
EndFunc
