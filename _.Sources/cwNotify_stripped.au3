$aTimePart = StringSplit($sDateTime[2], ":")
$nFields = UBound($aTimePart)
If ($nFields < 3) Or ($nFields > 4) Then Return SetError(2, 0, 0)
If $nFields < 4 Then
ReDim $aTimePart[4]
$aTimePart[3] = 0
EndIf
Else
Dim $aTimePart[4]
EndIf
For $x = 1 To 3
$aDatePart[$x] = Int($aDatePart[$x])
$aTimePart[$x] = Int($aTimePart[$x])
