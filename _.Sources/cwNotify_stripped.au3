$sDig_output&=@LF & ";; QUESTION SECTION:" & @LF & ";" & $vRet
Else
for $i = 1 to $vRet[0][0]
$aRet[0][0]+=1
ReDim $aRet[$aRet[0][0]+1][6]
$aRet[$aRet[0][0]][0]="Q"
$aRet[$aRet[0][0]][1]=$vRet[$i][0]
$aRet[$aRet[0][0]][2]=$vRet[$i][1]
$aRet[$aRet[0][0]][3]=$vRet[$i][2]
$aRet[$aRet[0][0]][4]=$vRet[$i][3]
$aRet[$aRet[0][0]][5]=$vRet[$i][4]
Next
EndIf
if @error Then
SetError(1)
return ""
EndIf
EndIf
if $iDig_a_count>0 Then
$vRet=ReadResourceRecords("a",$sDig_vRet)
if $sDig_vRet Then
$sDig_output&=@LF & ";; ANSWER SECTION:" & @LF & $vRet
Else
for $i = 1 to $vRet[0][0]
$aRet[0][0]+=1
ReDim $aRet[$aRet[0][0]+1][6]
$aRet[$aRet[0][0]][0]="A"
$aRet[$aRet[0][0]][1]=$vRet[$i][0]
$aRet[$aRet[0][0]][2]=$vRet[$i][1]
$aRet[$aRet[0][0]][3]=$vRet[$i][2]
$aRet[$aRet[0][0]][4]=$vRet[$i][3]
$aRet[$aRet[0][0]][5]=$vRet[$i][4]
Next
EndIf
if @error Then
SetError(1)
return ""
EndIf
EndIf
if $iDig_au_count>0 Then
$vRet=ReadResourceRecords("au",$sDig_vRet)
if $sDig_vRet Then
$sDig_output&=@LF & ";; AUTHORITY SECTION:" & @LF & $vRet
Else
for $i = 1 to $vRet[0][0]
$aRet[0][0]+=1
ReDim $aRet[$aRet[0][0]+1][6]
$aRet[$aRet[0][0]][0]="AU"
$aRet[$aRet[0][0]][1]=$vRet[$i][0]
$aRet[$aRet[0][0]][2]=$vRet[$i][1]
$aRet[$aRet[0][0]][3]=$vRet[$i][2]
$aRet[$aRet[0][0]][4]=$vRet[$i][3]
$aRet[$aRet[0][0]][5]=$vRet[$i][4]
Next
EndIf
if @error Then
SetError(1)
return ""
EndIf
EndIf
if $iDig_ar_count>0 Then
$vRet=ReadResourceRecords("ar",$sDig_vRet)
if $sDig_vRet Then
$sDig_output&=@LF & ";; ADDITIONAL SECTION:" & @LF & $vRet
Else
for $i = 1 to $vRet[0][0]
$aRet[0][0]+=1
ReDim $aRet[$aRet[0][0]+1][6]
$aRet[$aRet[0][0]][0]="AR"
$aRet[$aRet[0][0]][1]=$vRet[$i][0]
$aRet[$aRet[0][0]][2]=$vRet[$i][1]
$aRet[$aRet[0][0]][3]=$vRet[$i][2]
$aRet[$aRet[0][0]][4]=$vRet[$i][3]
$aRet[$aRet[0][0]][5]=$vRet[$i][4]
Next
EndIf
if @error Then
SetError(1)
return ""
EndIf
EndIf
if $sDig_vRet Then $sDig_output&=@LF & ";; Query time: " & $query_time & " msec" & @LF
if $sDig_vRet Then
$sDig_output&=";; WHEN: " & _Now() & @LF
$sDig_output&=";; MSG SIZE  rcvd: " & BinaryLen($bDig_amsg) & @LF
Return $sDig_output
Else
return $aRet
EndIf
EndFunc
Func ReadResourceRecords($section_id,$sDig_vRet=False)
Local $i,$count
Local $name_dec,$type_dec,$class_dec,$ttl_dec,$rd_len,$data_dec
Switch $section_id
case "q"
$count=$iDig_q_count
case "a"
$count=$iDig_a_count
case "au"
$count=$iDig_au_count
case "ar"
$count=$iDig_ar_count
case Else
SetError(1)
return ""
EndSwitch
Local $sRet=""
Local $aRet[1][5]
$aRet[0][0]=0
for $i=1 to $count
$name_dec=DecodeName()
$type_dec=DecodeType()
$class_dec=DecodeClass()
$ttl_dec=""
$data_dec=""
if $section_id<>"q" Then
$ttl_dec=ReadHex2Int(4)
$rd_len=ReadHex2Int(2)
$iDig_ptr_end=$iDig_ptr+$rd_len
$data_dec=DecodeRData($rd_len,$type_dec)
EndIf
if $sDig_vRet Then
$sRet&=$name_dec & @TAB & $ttl_dec & @TAB & $class_dec & @TAB & $type_dec & @TAB & $data_dec & @LF
Else
$aRet[0][0]+=1
ReDim $aRet[$aRet[0][0]+1][5]
$aRet[$aRet[0][0]][0]=$name_dec
$aRet[$aRet[0][0]][1]=$ttl_dec
$aRet[$aRet[0][0]][2]=$class_dec
$aRet[$aRet[0][0]][3]=$type_dec
$aRet[$aRet[0][0]][4]=$data_dec
EndIf
Next
if $sDig_vRet Then
return $sRet
Else
return $aRet
EndIf
EndFunc
Func EncodeName($sDig_domain)
Local $ret="",$i,$sDig_domain_array
$sDig_domain_array = StringSplit($sDig_domain, ".")
For $i = 1 To $sDig_domain_array[0]
$ret&=Hex(BinaryLen($sDig_domain_array[$i]), 2) & StringTrimLeft(StringToBinary($sDig_domain_array[$i]), 2)
Next
$ret&="00"
return $ret
EndFunc
Func DecodeName()
Local $ret
$ret=DecodeNameRec($iDig_ptr)
if $ret="" then $ret="."
$iDig_ptr+=@extended
return $ret
EndFunc
Func DecodeNameRec($offset)
local $data,$data_dec="",$offset_ptr,$len
Local $offset_add=0
while 1
$len=ReadHex2Int(1,$offset+$offset_add,False)
if BitAND(192,$len)=192 then
$offset_ptr=BitAND(ReadHex2Int(2,$offset+$offset_add,False),16383)+1
$data_dec&=DecodeNameRec($offset_ptr)
$offset_add+=2
ExitLoop
Else
$offset_add+=1
if $len=0 then ExitLoop
$data=BinaryMid($bDig_amsg,$offset+$offset_add,$len)
$data_dec&=_HexToString(StringMid($data,3)) & "."
$offset_add+=$len
EndIf
WEnd
SetExtended($offset_add)
Return $data_dec
EndFunc
Func EncodeType($rr_type)
Local $ret
$ret=StringRegExp(" " & $sDig_rr_types & " "," " & StringUpper($rr_type) & "=([0-9]+) ",1)
if not @error then
Return Hex($ret[0],4)
Else
if StringLeft(StringUpper($rr_type),4)="TYPE" Then
Return Hex(StringMid($rr_type,5),4)
Else
SetError(1)
return "00FF"
EndIf
EndIf
EndFunc
Func DecodeType($advance=True)
Local $ret=""
$rr_type=ReadHex2Int(2,$iDig_ptr,$advance)
$ret=StringRegExp(" " & $sDig_rr_types & " "," ([0-9,A-Z]+)=" & $rr_type & " ",1)
if not @error then
Return $ret[0]
Else
return "TYPE" & $rr_type
EndIf
EndFunc
Func EncodeClass($rr_class)
Local $ret
$ret=StringRegExp(" " & $sDig_rr_classes & " "," " & StringUpper($rr_class) & "=([0-9]+) ",1)
if not @error then
Return Hex($ret[0],4)
Else
if StringLeft(StringUpper($rr_class),5)="CLASS" Then
Return Hex(StringMid($rr_class,6),4)
Else
SetError(1)
return "00FF"
EndIf
EndIf
EndFunc
Func DecodeClass($advance=True)
Local $ret=""
$rr_class=ReadHex2Int(2,$iDig_ptr,$advance)
$ret=StringRegExp(" " & $sDig_rr_classes & " "," ([0-9,A-Z]+)=" & $rr_class & " ",1)
if not @error then
Return $ret[0]
Else
return "CLASS" & $rr_class
EndIf
EndFunc
Func DecodeRData($len,$type_dec,$advance=True)
local $ret="",$i,$data
Switch $type_dec
case "A"
$ret&=ReadHex2Int(1)
for $i=2 to 4
$ret&="." & ReadHex2Int(1)
Next
case "AAAA"
for $i=1 to 8
$data=StringRegExpReplace(Hex(BinaryMid($bDig_amsg,$iDig_ptr,2)),"^0{1,3}","")
$ret&=$data & ":"
$iDig_ptr+=2
Next
$ret=StringTrimRight(StringRegExpReplace($ret,"(:0)+",":",1),1)
case "MX"
$ret&=ReadHex2Int(2)
$ret&=" " & DecodeName()
case "CNAME","NS","PTR"
$ret=DecodeName()
case "SOA"
$ret&=DecodeName()
$ret&=" " & DecodeName()
$ret&=" " & ReadHex2Int(4)
$ret&=" " & ReadHex2Int(4)
$ret&=" " & ReadHex2Int(4)
$ret&=" " & ReadHex2Int(4)
$ret&=" " & ReadHex2Int(4)
case "SRV"
$ret&=ReadHex2Int(2)
$ret&=" " & ReadHex2Int(2)
$ret&=" " & ReadHex2Int(2)
$ret&=" " & DecodeName()
case "SPF","TXT"
$ret=''
$data=BinaryMid($bDig_amsg,$iDig_ptr,$len)
while BinaryLen($data)>0
$string_len=int("0x"&Hex(BinaryMid($data,1,1)))
$ret&='"' & BinaryToString(BinaryMid($data,2,$string_len)) & '" '
$data=BinaryMid($data,$string_len+2)
WEnd
$ret=StringTrimRight($ret,1)
$iDig_ptr+=$len
case Else
$ret="\# " & $len & " " & hex(BinaryMid($bDig_amsg,$iDig_ptr,$len))
$iDig_ptr+=$len
EndSwitch
Return $ret
EndFunc
Func ReadHex2Int($len,$offset=$iDig_ptr,$advance=True)
Local $ret
$ret=Int("0x"&hex(BinaryMid($bDig_amsg,$offset,$len)))
if $advance then $iDig_ptr+=$len
return $ret
EndFunc
Func GetPrimaryDNS()
Local $ret
Local $colNICs="", $NIC, $strQuery, $objWMIService
$strQuery = "SELECT * FROM Win32_NetworkAdapterConfiguration"
$objWMIService = ObjGet("winmgmts:\\.\root\CIMV2")
$colNICs = $objWMIService.ExecQuery($strQuery, "WQL", 0x30)
If IsObj($colNICs) Then
For $NIC In $colNICs
if $NIC.IPAddress(0)==@IPAddress1 then
$ret=$NIC.DNSServerSearchOrder(0)
ExitLoop
EndIf
Next
Else
SetError(-1, "No WMI Objects Found for class: Win32_NetworkAdapterConfiguration", "")
EndIf
$objWMIService = ""
$colNICs = ""
$NIC = ""
Return $ret
EndFunc
Global $g_aResolveSrvDNS[0][2]
Global $g_aResolveQueryDNS[0]
Global $g_aResolveAdapters
Func _nResolve($sQuery,$bReverse=False,$bFullSearch=False,$iTimeout=1000)
Local $aResolve[0][2], $sHost, $aAnswer, $sDigQuery, $bHaveResolve=False
If UBound($g_aResolveQueryDNS,1)==0 Or UBound($g_aResolveSrvDNS,1)==0 Then
_nResolveUpdateInfoDNS()
If UBound($g_aResolveSrvDNS,1)==0 Then Return SetError(1,0,False)
If UBound($g_aResolveQueryDNS,1)==0 Then Return SetError(2,0,False)
EndIf
For $i=0 To UBound($g_aResolveSrvDNS,1)-1
If not $g_aResolveSrvDNS[$i][1] Then
If UBound($g_aResolveSrvDNS,1)-1==$i Then Return SetError(3,0,False)
ContinueLoop
EndIf
If Not $bReverse Then
For $j=0 To UBound($g_aResolveQueryDNS,1)-1
If StringInStr($sQuery,'.'&$g_aResolveQueryDNS[$j]) Then $sQuery=StringReplace($sQuery,'.'&$g_aResolveQueryDNS[$j],"")
If StringRight($sQuery,1)=='.' Then $sQuery=StringTrimRight($sQuery,1)
Next
EndIf
For $j=0 To UBound($g_aResolveQueryDNS,1)-1
If Not $bFullSearch And $bHaveResolve Then ExitLoop
If $bReverse Then
$sDigQuery=StringRegExpReplace($sQuery,"([0-9]+)\.([0-9]+)\.([0-9]+)\.([0-9]+)","\4\.\3\.\2\.\1") & ".in-addr.arpa"
Else
$sDigQuery=$sQuery&'.'&$g_aResolveQueryDNS[$j]
EndIf
$sbRev=$bReverse ? "PTR" : "A"
ConsoleWrite('nDig('&$sDigQuery&','&$g_aResolveSrvDNS[$i][0]&','&$sbRev&','&$iTimeout&')'&@CRLF)
$aRet=_nDig($sDigQuery,$g_aResolveSrvDNS[$i][0],$bReverse ? "PTR" : "A", $iTimeout)
If @error Then
ContinueLoop
EndIf
For $k=0 To UBound($aRet,1)-1
If $aRet[$k][0]<>'A' Then ContinueLoop
If $bReverse Then
$sHost=$sQuery
If StringRight($aRet[$k][5],1)=='.' Then $aRet[$k][5]=StringTrimRight($aRet[$k][5],1)
Else
If StringRight($aRet[$k][1],1)=='.' Then $aRet[$k][1]=StringTrimRight($aRet[$k][1],1)
$sHost=$aRet[$k][1]
EndIf
$iMax=UBound($aResolve,1)
If $iMax>0 Then
For $l=0 To $iMax-1
If $sHost==$aResolve[$l][0] Then
If $aRet[$k][5]==$aResolve[$l][1] Then ContinueLoop 2
If IsArray($aResolve[$l][1]) Then
$aAnswer=$aResolve[$l][1]
$iMaxA=UBound($aAnswer,1)
ReDim $aAnswer[$iMaxA+1]
$aAnswer[$iMaxA]=$aRet[$k][5]
Else
Dim $aAnswer[2]
$aAnswer[0]=$aResolve[$l][1]
$aAnswer[1]=$aRet[$k][5]
EndIf
