Func _Toast_Set($vJust, $iHdr_BkCol = -1, $iHdr_Col = -1, $iMsg_BkCol = -1, $iMsg_Col = -1, $iFont_Size = -1, $sFont_Name = "", $iTime_Out = 1000, $iTime_In = 500)
Switch $vJust
Case Default
$iToast_Style = 1
$aRet = DllCall("User32.dll", "int", "GetSysColor", "int", 8)
