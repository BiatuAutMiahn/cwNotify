StringRegExp($s_String, $s_RE_G_Array_End, 1, $i_OsC)
If Not @error Then
$i_OsC = @extended
ExitLoop
Else
Return SetError(5, $i_OsC, "")
EndIf
EndIf
Until False
If UBound($o_Current) <> $i_C Then ReDim $o_Current[$i_C]
Return SetExtended($i_OsC, $o_Current)
EndIf
$o_Current = StringRegExp($s_String, $s_RE_G_Number, 1, $i_Os)
If Not @error Then Return SetExtended(@extended, Number(StringLower($o_Current[0])))
$o_Current = StringRegExp($s_String, $s_RE_G_KeyWord, 1, $i_Os)
If Not @error Then Return SetExtended(@extended, Execute($o_Current[0]))
